        ["uint", "m.Uint"],
        ["Int8", "m.Int8"],
        ["Uint8", "m.Uint8"],
        ["Sint8", "m.Int8"],
        ["Uint8*", "*m.Uint8"],
        ["Int16", "m.Int16"],
        ["Uint16", "m.Uint16"],
        ["Sint16", "m.Int16"],
        ["Uint16*", "*m.Uint16"],
        ["Int32", "m.Int32"],
        ["Uint32", "m.Uint32"],
        ["Sint32", "m.Int32"],
        ["float", "m.Float"],
        ["double", "m.Double"],
        ["size_t", "m.SizeT"],

        ["SDL_bool", "*m.Bool"],
        ["SDL_RWops*", "*m.RWops"],
        ["SDL_version*", "*m.Version"],

        ["Mix_Chunk*", "*mm.Chunk"],
